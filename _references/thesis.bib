@inproceedings{RDS+20,
  title = {Iterative {{Tensor Receiver}} for {{MIMO-GFDM}} Systems},
  booktitle = {Proc. 11th {{IEEE Sensor Array}} and {{Multichannel Signal Processing Workshop}} ({{SAM}} 2020)},
